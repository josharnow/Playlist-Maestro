{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": ["."],
    "includePattern": ".(jsx|js|ts|tsx)$"
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs",
    "readme": "README.md"
  }
}